        <div class="col-8 scrollable-menu rightPane">

            <!-- This place can be filled with teacher supplied text. We should probably still sanitize this input somehow. -->
            <div class="jumbotron">
                <h1>@Html.DisplayFor(model => model.moduleTitle)</h1>
                <p>@Html.DisplayFor(model => model.lectureText)</p>
            </div>
            <!-- We can put a teacher supplied Youtube link here to display it. -->
            <div class="YoutubeVideo">
                <iframe src="@Html.DisplayFor(model => model.videoURL)" frameborder="0" allowfullscreen></iframe>
            </div>

            <!-- We can put a teacher supplied mp4/ogg file here to display it, currently it must be name "Video.mp4" or "Video.ogg -->
            <div class="UploadVideo">
                <video controls>
                    <source src="Video.mp4" type="video/mp4">
                    <source src="Video.ogg" type="video/ogg">
                    Your browser does not support the video tag.
                </video>
            </div>
